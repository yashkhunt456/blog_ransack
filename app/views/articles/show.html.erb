<h1>Articles</h1>
<h1><%= @article.title %></h1>

<p><%= @article.content %></p>
<%= link_to "Back", root_path %>
<% if authenticated? %>
    <%= link_to "Edit", edit_article_path(@article) %>
    <%= link_to "destroy", article_path(@article), method: :delete, data: { turbo_cunfirm: "Are you sure?" } %>
<% end %>